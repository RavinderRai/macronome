{
  "cli": {
    "version": ">= 16.28.0",
    "appVersionSource": "remote"
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal"
    },
    "preview": {
      "distribution": "internal",
      "env": {
        "NPM_CONFIG_LEGACY_PEER_DEPS": "true",
        "NODE_ENV": "production",
        "API_BASE_URL": "http://macronome.ravinderrai.com:8000",
        "CLERK_PUBLISHABLE_KEY": "${CLERK_PUBLISHABLE_KEY}"
      }
    },
    "production": {
      "autoIncrement": true,
      "env": {
        "NPM_CONFIG_LEGACY_PEER_DEPS": "true",
        "NODE_ENV": "production",
        "API_BASE_URL": "http://macronome.ravinderrai.com:8000",
        "CLERK_PUBLISHABLE_KEY": "${CLERK_PUBLISHABLE_KEY}"
      }
    }
  },
  "submit": {
    "production": {}
  }
}
